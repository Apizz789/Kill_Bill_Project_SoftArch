import "org/killbill/billing/util/entity/dao/EntitySqlDao.sql.stg"

tableName() ::= "kombucha"

historyTableName() ::= "kombucha_history"

tableFields(prefix) ::= <<
  <prefix>tea
, <prefix>mushroom
, <prefix>sugar
>>

